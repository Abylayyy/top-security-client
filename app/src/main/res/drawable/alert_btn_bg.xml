<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- disable state. -->
    <item android:state_enabled="false" android:drawable="@drawable/alert_btn_pressed"/>

    <!--released state. -->
    <item android:state_pressed="false" android:drawable="@drawable/alert_btn_normal"/>

    <!--pressed state. -->
    <item android:state_pressed="true" android:drawable="@drawable/alert_btn_pressed"/>

</selector>
